jQuery(function(){showHideShipping(),jQuery("input.sameAsBilling").change(function(){jQuery("#sameAsBilling").click()}),jQuery("#billing_state").change(function(){check_address("billing")}),jQuery("#save_address").change(function(){check_address("shipping")}),jQuery("#shipping_country").change(function(){var n=jQuery(this).data("CustomerInfo_state");populateState("shipping_state","shipping_country","",!0,n),check_stateValidator("billing"),check_address("billing")})})