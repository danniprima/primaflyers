function DeleteMultipleList(n){str="multiple_wishlist_"+n,confirm(getVariableVal("wishlist_multiple-message-deletelist"))&&(document.frmForm.action.value="removeList",document.frmForm.k.value=n,frmForm.submit())}function SaveMultipleList(n){if(str="multiple_wishlist_"+n,WshName=document.getElementById(str).value,strMsg="",WshName.trim()==""&&(strMsg+=getVariableVal("wishlist_multiple-message-renamelist")),String.prototype.trim=function(){return this.replace(/^\s+|\s+$/g,"")},strMsg!="")return alert(strMsg),!1;document.frmForm.action.value="saveList",document.frmForm.k.value=n,document.frmForm.multiple_wishlist_name.value=WshName,frmForm.submit()}function RenameMultipleList(n,t){Spanlabel="multiple_wishlist_label_"+n,SpanLink="multiple_wishlist_textbox_"+n,t==1?(document.getElementById(SpanLink).style.display="block",document.getElementById(Spanlabel).style.display="none"):(document.getElementById(SpanLink).style.display="none",document.getElementById(Spanlabel).style.display="block")}function submitForm(n){window.document.body.style.cursor="wait",window.document.activeElement&&(window.document.activeElement.style.cursor="wait"),document.frmForm.action.value=n,document.frmForm.submit()}function removeItem(n,t){var i;if(i=confirm(getVariableVal("wishlist_viewwishlist-message-removeitem")),!i)return!1;document.frmForm.wsiId.value=n,document.frmForm.k.value=t,submitForm("removeItem")}function addToCart(n,t){var i;if(i=confirm(getVariableVal("wishlist_viewwishlist-message-addtocart")),!i)return!1;document.frmForm.wsiId.value=n,document.frmForm.k.value=t,submitForm("addToCart")}function saveItem(n,t){var i=document.getElementById("txtQty"+n).value;document.frmForm.wsiId.value=n,document.frmForm.k.value=t,document.frmForm.qty.value=i,submitForm("saveItem")}function savePublicOrPrivate(n){var t="";t=document.getElementById("optListVisibilityPublic").checked==!0?"1":"0",document.frmForm.optListVisibility.value=t,document.frmForm.action.value="savePublicOrPrivate",document.frmForm.k.value=n,frmForm.submit()}